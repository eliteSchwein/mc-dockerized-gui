{
  "name": "mc-dockerized-gui",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "serve": "./node_modules/.bin/vue-cli-service serve",
    "build": "./node_modules/.bin/vue-cli-service build",
    "lint": "./node_modules/.bin/vue-cli-service lint",
    "lint-cleanup": "npx eslint ./** --fix"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "core-js": "^3.7.0",
    "echarts": "^5.1.1",
    "echarts-gl": "^2.0.4",
    "eslint-config-galex": "^2.15.9",
    "monaco-editor": "^0.24.0",
    "monaco-editor-textmate": "^2.2.2",
    "monaco-textmate": "^3.0.1",
    "monaco-themes": "^0.3.3",
    "object-assign-deep": "^0.4.0",
    "vue": "^2.6.12",
    "vue-context": "^6.0.0",
    "vue-headful": "^2.1.0",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.4.9",
    "vue-simple-upload": "^0.1.6",
    "vue-toast-notification": "^0.6.0",
    "vuetify": "^2.3.18",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@mdi/font": "^5.8.55",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.3",
    "@vue/cli-plugin-pwa": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "babel-eslint": "^10.1.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.9.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "sw-precache-webpack-plugin": "^1.0.0",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.6.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "vue/custom-event-name-casing": [
    "error",
    "camelCase",
    {
      "ignores": [
        "onInputChange",
        "onInputFocus",
        "/^[a-z]+(?:-[a-z]+)*:[a-z]+(?:-[a-z]+)*$/u"
      ]
    }
  ]
}
